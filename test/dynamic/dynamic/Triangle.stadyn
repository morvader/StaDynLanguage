
using System;

class Triangle {
    public var x1, y1, x2, y2, x3, y3;
    public Triangle() {
        x1 = Random.Next(-20, 20);
        y1 = Random.Next(-20, 20);
        x2 = Random.Next(-20, 20);
        y2 = Random.Next(-20, 20);
        x3 = Random.Next(-20, 20);
        y3 = Random.Next(-20, 20);
    }
}

